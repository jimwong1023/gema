// Animated Chart
@mixin stagger-anim-delay($i, $initial-delay, $stagger-delay) {
  @while $i > 0 {
    &:nth-of-type(#{$i}) {
      animation-delay: $initial-delay + $stagger-delay * $i;
    }
    $i: $i - 1;
  }
}

.moh {
  width: 90%;
  max-width: 960px;
  height: 250px;

  position: relative;
  
  @media only screen and (max-width: $grid-medium) {
    width: auto;
  }
  
  .legend-right {
    float: right;
  }
}

.charts {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
}

.chart {
  margin: 30px 0 0;

  &:first-child {
    margin: 0;
  }
}

.chart__title {
  display: block;
  margin: 0 0 10px;

  opacity: 0;

  animation: 1s anim-lightspeed-in ease forwards;

  .chart--dev & {}
  .chart--prod & {
    animation-delay: 3.3s;
  }
  .chart--design & {
    animation-delay: 4.5s;
  }
}

.chart--horiz {
  overflow: hidden;
  border-right: 2px dotted #808284;
  border-left: 2px dotted #808284;
}

.chart__bar {
  $border-rad: 4px;
/*   height: 35px; */
  margin-bottom: 16px;
  background: #CDA490;
  border-top-right-radius: none;
  border-bottom-right: none;
  border-top: 2px solid #CDA490;
  border-right: 2px solid #CDA490;
  border-bottom: 2px solid #CDA490;

/*   background: linear-gradient(to left, #4cb8c4, #3cd3ad);
  border-top-right-radius: $border-rad;
  border-bottom-right-radius: $border-rad; */
  opacity: 0;

  animation: 0.5s anim-lightspeed-in ease forwards;
}
.chart__bar_secondary {
  color: #CDA490;
  background: #FFFFFF;
}
.chart__label {
  padding-left: 15px;
  line-height: 35px;
  color: white;
}
.chart__label_secondary,
.chart__value_secondary {
  color: #CDA490 !important;
}
.chart__value {
  float: right;
  line-height: 35px;
  color: #FFFFFF;
  padding-right: 15px;
}

//Keyframes
@keyframes anim-lightspeed-in {
  0% {
    transform: translateX(-200%);
    opacity: 1;
  }
  100% {
    transform: translateX(0);
    opacity: 1;
  }
}
// End Animated Chart

