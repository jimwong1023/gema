.c-layered-section__image-left {
  max-width: 100%;
  overflow: hidden;
  box-sizing: inherit;
  /*   accounts for copy-box space */
  margin-bottom: -600px;
  
  @media only screen and (max-width: 1215px) {
    margin-bottom: -150px;
  }
  
  .c-layered-section__image-box-wrapper {
    margin-right: 40%;
    margin-left: 0;
    position: relative;
    display: inline-block;
    vertical-align: middle;
    width: 55%;
    min-height: 700px;
    height: 700px;
    overflow: hidden;
    z-index: 0;
    
    @media only screen and (max-width: 1215px) {
      display: block;
      margin: 0 0 0 0;
      width: 100%;
    }

    img {
      width: 100%;
      height: 100%;
      max-height: 100%;
      object-fit: cover;
    }
  }

  .c-layered-section__copy-box-wrapper {
    transform: translate3d(550px, -625px, 0px);
    position: relative;
    display: inline-block;
    vertical-align: middle;
    width: 49%;
    margin-right: 4.7619%;
    margin-left: auto;
    z-index: 2;

    @media only screen and (max-width: 1215px) {
      display: block;
      margin: auto;
      width: 80%;
      transform: translate3d(0px, -150px, 0px);
    }

    .copy-box {
      background: #ffffff;
      min-height: 550px;
      max-height: 550px;
      overflow: hidden;

      @media only screen and (max-width: $grid-large) {
        max-height: 100%;
        min-height: 450px;
      }
    }
    
    .copy-box-padding {
      padding: 60px;
      
      @media only screen and (max-width: $grid-large) {
        padding: 20px;
      }
    }
  }
}

